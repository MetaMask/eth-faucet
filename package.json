{
  "name": "eth-faucet",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "index.js",
  "browserify": {
    "transform": [
      "envify"
    ]
  },
  "dependencies": {
    "@pablopunk/geo-ip": "^1.0.0",
    "body-parser": "^1.15.2",
    "cors": "^2.8.1",
    "emoji-flag": "^1.1.0",
    "eth-query": "^2.1.2",
    "ethereumjs-tx": "^2.1.2",
    "ethereumjs-util": "^7.0.0",
    "ethjs-query": "^0.3.8",
    "express": "^4.14.0",
    "express-rate-limit": "^2.5.0",
    "lavamoat": "^2.0.0",
    "patch-package": "^6.2.2",
    "request": "^2.75.0",
    "web3": "^1.0.0-beta.20",
    "web3-provider-engine": "^15.0.6",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "browserify": "^16.2.3",
    "concurrently": "^5.2.0",
    "envify": "^4.0.0",
    "ethers": "^4.0.47",
    "ganache-cli": "^6.9.1",
    "h": "^0.1.0",
    "postinstall-postinstall": "^2.1.0",
    "standard": "^14.3.3",
    "tape": "^5.0.0",
    "xhr": "^2.2.2"
  },
  "scripts": {
    "test": "concurrently --kill-others --success=first 'npm:test:ganache' 'npm:test:server' 'npm:test:runner'",
    "test:ganache": "ganache-cli --defaultBalanceEther '1000000000' -m 'stool sorry reveal vacuum notice grass trap aspect trigger marble mountain cargo'",
    "test:server": "sleep 3 && FAUCET_CONFIG='test' FAUCET_CONFIG_PATH='test/config.js' npm start",
    "test:runner": "sleep 7 && node test/index.js",
    "test:stress": "SKIP_RATE_LIMITER=true concurrently --kill-others --success=first 'npm:test:ganache' 'npm:test:server' 'npm:test:stress:runner'",
    "test:stress:runner": "sleep 7 && node test/stress.js",
    "start": "lavamoat src/server.js",
    "build": "node src/build.js > build/app.js",
    "deploy": "docker-compose pull && docker-compose build && docker-compose stop && docker-compose up -d && docker-compose logs -f --tail 200",
    "postinstall": "yarn run postinstall:patch && yarn run postinstall:scripts",
    "postinstall:patch": "patch-package",
    "postinstall:scripts": "yarn run postinstall:scripts:keccak && yarn run postinstall:scripts:keccak2 && yarn run postinstall:scripts:secp256k1",
    "postinstall:scripts:keccak": "cd node_modules/keccak && yarn run install",
    "postinstall:scripts:keccak2": "cd node_modules/ethereumjs-abi/node_modules/keccak && yarn run install",
    "postinstall:scripts:secp256k1": "cd node_modules/secp256k1 && yarn run install"
  },
  "author": "",
  "license": "ISC"
}
